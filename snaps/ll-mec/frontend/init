#!/bin/sh

. $SNAP/common

if [ ! -f $CONF_DIR/llmec_config.json ] ; then
    cp $SNAP/etc/* $CONF_DIR

# set the conf
$SNAP/conf set $CONF_DIR/llmec_config.json

# a copy for the user
if [ ! -f $SNAP_USER_DATA/llmec_config.json ] ; then 
    cp $CONF_DIR/llmec_config.json $SNAP_USER_DATA
fi


echo "LLMEC init: done "
